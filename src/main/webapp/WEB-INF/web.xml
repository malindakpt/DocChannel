<web-app version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
	http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

	<display-name>Spring MVC Application</display-name>

    <servlet>
        <servlet-name>getChannelSessionsServlet</servlet-name>
        <servlet-class>com.servlet.getChannelSessionsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getChannelSessionsServlet</servlet-name>
        <url-pattern>/getChannelSessionsServlet</url-pattern>
    </servlet-mapping>




	<servlet>
		<servlet-name>getDoctorListOfHospitalServlet</servlet-name>
		<servlet-class>com.servlet.getDoctorListOfHospitalServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getDoctorListOfHospitalServlet</servlet-name>
		<url-pattern>/getDoctorListOfHospitalServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>createChannelServlet</servlet-name>
		<servlet-class>com.servlet.createChannelServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createChannelServlet</servlet-name>
		<url-pattern>/createChannelServlet</url-pattern>
	</servlet-mapping>



    <servlet>
		<servlet-name>createDoctorServlet</servlet-name>
		<servlet-class>com.servlet.createDoctorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createDoctorServlet</servlet-name>
		<url-pattern>/createDoctorServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>mvc-dispatcher</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>mvc-dispatcher</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
</web-app>